{
  "name": "arbor",
  "version": "0.3.1",
  "description": "A simple CLI tool to build projects across several platforms in parallel",
  "main": "dist/index.js",
  "bin": {
    "arbor": "dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "build": "tsc --project tsconfig.json",
    "build--prod": "npm run clean && npm run lint && npm run build",
    "lint": "tslint \"src/**/*.ts\"",
    "ide-build": "yarn run clean && yarn run lint && yarn run build && echo successfully built"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vintage-software/arbor.git"
  },
  "keywords": [
    "Build",
    "Tool",
    "Build",
    "CLI"
  ],
  "author": "Cory Rylan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vintage-software/arbor/issues"
  },
  "homepage": "https://github.com/vintage-software/arbor#readme",
  "dependencies": {
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "dependency-graph": "0.5.0"
  },
  "devDependencies": {
    "@types/chalk": "0.4.31",
    "@types/commander": "2.3.31",
    "@types/node": "6.0.56",
    "rimraf": "2.5.4",
    "tslint": "4.4.2",
    "typescript": "2.1.6"
  }
}
